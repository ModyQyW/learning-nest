{
  "name": "learning-nest",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production nest build",
    "commit": "git-cz",
    "predev": "rimraf dist",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "postinstall": "npx tsx ./scripts/docker-compose-up.mts",
    "lint": "eslint . --ext=.js,.cjs,.mjs.jsx,.ts,.cts,.mts,.tsx --fix --ignore-path=.gitignore && tsc --noEmit",
    "prepare": "is-ci || husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main",
    "test": "jest",
    "posttest": "docker-compose stop test-db && docker-compose rm -f test-db",
    "test:cov": "pnpm run test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run test --config ./test/jest-e2e.json",
    "test:watch": "pnpm run test --watch"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.6",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/mongoose": "^9.1.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/throttler": "^2.0.1",
    "bull": "^4.8.3",
    "cache-manager": "^4.0.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.3.6",
    "nanoid": "^4.0.0",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "^9.1.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "picocolors": "^1.0.0",
    "ramda": "^0.28.0",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.4.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/prompt": "^17.0.0",
    "@modyqyw/fabric": "^5.2.0",
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.6",
    "@types/bull": "^3.15.8",
    "@types/cache-manager": "^4.0.0",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "latest",
    "@types/passport-local": "^1.0.34",
    "@types/ramda": "^0.28.14",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tsx": "^3.4.2",
    "type-fest": "^2.13.0",
    "typescript": "^4.7.3"
  }
}

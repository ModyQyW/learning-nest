{
  "name": "learning-nest",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env MODE=production NODE_ENV=production nest build",
    "build:development": "cross-env MODE=development NODE_ENV=production nest build",
    "build:production": "pnpm run build",
    "build:staging": "cross-env MODE=staging NODE_ENV=production nest build",
    "commit": "cz",
    "predev": "rimraf dist",
    "dev": "cross-env MODE=development NODE_ENV=development nest start --watch",
    "dev:development": "cross-env MODE=development NODE_ENV=development nest start --watch",
    "dev:production": "cross-env MODE=production NODE_ENV=development nest start --watch",
    "dev:staging": "cross-env MODE=staging NODE_ENV=development nest start --watch",
    "docker": "npx tsx ./scripts/docker.mts",
    "docker-scale": "npx tsx ./scripts/docker-scale.mts",
    "docker-scale:test": "npx tsx ./scripts/docker-scale.test.mts",
    "docker:test": "npx tsx ./scripts/docker.test.mts",
    "lint": "run-p lint:eslint check:types",
    "lint:eslint": "eslint . --ext=.js,.cjs,.mjs.jsx,.ts,.cts,.mts,.tsx --fix --ignore-path=.gitignore --cache",
    "check:types": "tsc --noEmit",
    "check:deps": "ncu",
    "prepare": "is-ci || husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main",
    "test": "jest",
    "test:cov": "pnpm run test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run test --config ./test/jest-e2e.json",
    "test:watch": "pnpm run test --watch"
  },
  "dependencies": {

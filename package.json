    "@typescript-eslint/parser": "^5.33.1",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
